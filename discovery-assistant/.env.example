# Supabase Configuration
# Get these values from your Supabase project settings
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# AI API Configuration (optional)
# Configure your preferred AI provider
VITE_AI_PROVIDER=openai # or anthropic, cohere, etc.
VITE_AI_API_KEY=your-api-key
VITE_AI_MODEL=gpt-4 # or claude-3, command-r, etc.
VITE_AI_MAX_TOKENS=4000
VITE_AI_TEMPERATURE=0.7

# Application Settings
VITE_APP_NAME=Discovery Assistant
VITE_APP_VERSION=1.0.0
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_AI_FEATURES=false
VITE_ENABLE_COLLABORATION=false

# Feature Flags
VITE_FEATURE_WIZARD_MODE=true
VITE_FEATURE_CUSTOM_ENTRIES=true
VITE_FEATURE_EXPORT_PDF=true
VITE_FEATURE_EXPORT_EXCEL=true

# Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info