{
  "name": "discovery-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "server": "node server/openaiProxy.js",
    "build": "vite build",
    "build:typecheck": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:all": "vitest run && playwright test",
    "test:api": "node test-api-compatibility.mjs",
    "validate:api": "node validate-api-structure.mjs",
    "validate:prefill": "node scripts/validate-prefill.js",
    "validate:prefill:fix": "node scripts/validate-prefill.js --fix",
    "postinstall": "node ./scripts/copy-ffmpeg-assets.cjs"
